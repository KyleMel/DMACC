@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model CIS172_TestCoreAppMelton.Models.AssignmentViewModel

@if(Model.AccessLevel >= 8)
{
    <p>Hello Admin</p>
    foreach(var student in Model.Students)
    {
        <p>@student.FirstName @student.LastName - @student.Grade</p>
    }
}
else if(Model.AccessLevel > 2)
{
    foreach(var student in Model.Students)
    {
        <p>@student.FirstName @student.LastName</p>
    }
}
else
{
    <p>You do not have a sufficient access level to view this data.</p>
}